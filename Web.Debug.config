<?xml version="1.0" encoding="utf-8"?>

<!-- 有关使用 web.config 转换的详细信息，请访问 https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    在下例中，“SetAttributes”转换将更改 
    “connectionString”的值，以仅在“Match”定位器 
    找到值为“MyDB”的特性“name”时使用“ReleaseSQLServer”。
    
    <connectionStrings>
      <add name="MyDB" 
        connectionString="Data Source=ReleaseSQLServer;Initial Catalog=MyReleaseDB;Integrated Security=True" 
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  -->
	<appSettings>
		<add key="TRANSFER_ENDPOINT"      value="127.0.0.1:2021" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="SSO_LOGIN_URL"          value="http://test-sso.szprl.com/Validate.ashx" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="SSO_LOGOUT_URL"         value="http://test-sso.szprl.com/logout.aspx" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="SSO_TICKET_SERVICE_URL" value="http://test-sso.szprl.com/Ticket.asmx" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="SSO_MODULE_IGNORE_PATH" value="/sso/;/awake;" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="SERVICE_ID"             value="f161c303fdef" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="SECRET_KEY"             value="Nkha@DAZ" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="ACCESS_TICKET_COOKIE"   value="AccessTicketCookie" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="AUTH_COOKIES"           value="" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="LOGIN_URL"              value="/sso/login?ReturnUrl={ReturnUrl}" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
		<add key="ENABLE_LOGGING"         value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
	</appSettings>
  <system.web>
    <!--
      
      在下例中，“Replace”转换将替换 
      web.config 文件的整个 <customErrors> 节。
      请注意，由于 
      在 <system.web> 节点下仅有一个 customErrors 节，因此不需要使用“xdt:Locator”特性。
      
      <customErrors defaultRedirect="GenericError.htm"
        mode="RemoteOnly" xdt:Transform="Replace">
        <error statusCode="500" redirect="InternalError.htm"/>
      </customErrors>
    -->
  </system.web>
</configuration>